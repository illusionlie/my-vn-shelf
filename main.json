    "cover_image": "images/amazing-grace.jpg",
    "vndb_url": "https://vndb.org/v23448",
    "tags": ["长时长", "轮回", "悬疑", "佳作"],
    "rating_overall": 8.0,
    "rating_details": {
      "剧情": 8,
